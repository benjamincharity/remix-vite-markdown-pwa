  },
  "scripts": {
    "build": "pnpm run build-tw && run-s build:*",
    "build-tw": "cross-env NODE_ENV=production npx tailwindcss -i app/styles/shared.css -o public/shared.css --minify",
    "build:remix": "remix build",
    "build:worker": "remix-pwa build",
    "dev": "run-p dev:*",
    "dev:css": "npx tailwindcss -i app/styles/shared.css -o public/shared.css",
    "dev:remix": "remix dev --manual",
    "dev:worker": "remix-pwa dev",
    "e2e": "playwright test",
    "format": "npx prettier --log-level warn --write .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "start:prod": "cross-env NODE_ENV=production remix-serve ./build/index.js",
    "tailwind-watch": "tailwindcss build -i app/styles/shared.css -o public/shared.css --watch",
    "test": "jest --config jest.config.ts",
    "typecheck": "tsc"
  },
  "sideEffects": false,
  "type": "module",
  "version": "1.0.0"
